{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Millie: Start Milvus",
            "type": "debugpy",
            "request": "launch",
            "module": "millie",
            "args": [
                "milvus",
                "start"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Millie: Start Attu",
            "type": "debugpy",
            "request": "launch",
            "module": "millie",
            "args": [
                "attu",
                "start"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Millie: Create Migration",
            "type": "debugpy",
            "request": "launch",
            "module": "millie",
            "args": [
                "migrate",
                "create",
                "vs_code_task"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Millie: Seed Data",
            "type": "debugpy",
            "request": "launch",
            "module": "millie",
            "args": [
                "db",
                "seed"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: Pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${file}"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}